{
  "name": "@fathym/react-atomic",
  "version": "0.0.9",
  "main": "build/index.js",
  "module": "build/index.js",
  "homepage": ".",
  "scripts": {
    "build": "concurrently \"npm:build:*\"",
    "build_:react": "react-scripts build",
    "build:react": "tsc --build \"./tsconfig.json\"",
    "build:storybook": "storybook build -s public",
    "deploy": "npm version patch && npm run deploy:app",
    "deploy:app": "npm version patch --prefix public && npm run build && concurrently \"npm:publish:*\"",
    "publish:react": "npm publish ./build --access public",
    "publish:storybook": "npm run rename:storybook && npm publish ./storybook-static --access public",
    "rename:storybook": "node ./.storybook/storybook-rename.js",
    "fathym": "node ./fathym.js",
    "start": "concurrently \"npm:start:*\"",
    "start_:react": "react-scripts start",
    "start:storybook": "storybook dev -p 6006 -s public",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@semanticjs/common": "^0.56.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.8",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.9",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.0.0-beta.12",
    "@storybook/addon-essentials": "^7.0.0-beta.12",
    "@storybook/addon-interactions": "^7.0.0-beta.12",
    "@storybook/addon-links": "^7.0.0-beta.12",
    "@storybook/node-logger": "^7.0.0-beta.12",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^7.0.0-beta.12",
    "@storybook/react-webpack5": "^7.0.0-beta.12",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/theming": "^6.5.14",
    "autoprefixer": "^10.4.13",
    "babel-plugin-named-exports-order": "^0.0.2",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "npm": "^9.2.0",
    "postcss": "^8.4.20",
    "prop-types": "^15.8.1",
    "rimraf": "^3.0.2",
    "storybook": "^7.0.0-beta.12",
    "storybook-tailwind-dark-mode": "^1.0.15",
    "tailwindcss": "^3.2.4",
    "webpack": "^5.75.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
